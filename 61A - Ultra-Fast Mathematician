#include<stdio.h>
#include<math.h>
#include<string.h>
#include<stdlib.h>
int main()
{
    char a[105],b[105];
    int i;
    scanf("%s", &a);
    scanf("%s", &b);
    for(i=0;i<strlen(a);i++)
    {
        if(abs(a[i]-b[i])==0)
            a[i]='0';
        else
            a[i]='1';
    }
    printf("%s", a);
    return 0;
}
