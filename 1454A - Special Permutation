#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,t;
    cin >> t;
    while(t--)
    {
        cin >> n;
        if(n%2==0)
        {
            for(int i=n; i>0; i--)
                cout << i << " ";
            cout << "\n";
        }
        else
        {
            for(int i=n; i>(n/2)+1; i--)
                cout << i << " ";
            for(int i=1; i<(n/2)+2; i++)
                cout << i << " ";
            cout << "\n";
        }
    }
    return 0;
}
