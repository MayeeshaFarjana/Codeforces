#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        long long n;
        cin >> n;
        if(n<=6)
            cout << "15\n";
        else
        {
            if(n%2==0)
            {
                n/=2;
                cout<< n*5 <<endl;
            }
            else
            {
                n++;
                n/=2;
                cout<< n*5 <<endl;
            }
        }
    }
    return 0;
}
