#include<stdio.h>
#include<string.h>
int main()
{
    char s[105];
    int i,cnt=0;
    scanf("%s", &s);
    for(i=0; i<strlen(s); i++)
    {
        if(s[i]=='H' || s[i]=='Q' || s[i]=='9')
        {
            cnt++;
        }
    }
    if(cnt==0)
        printf("NO\n");
    else
        printf("YES\n");
    return 0;
}
