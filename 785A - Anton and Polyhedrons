#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n, sum = 0, i;
    string s;
    cin >> n;
    while(n--)
    {
        cin >> s;
        if(s=="Tetrahedron")
            i=4;
        if(s=="Cube")
            i= 6;
        if(s=="Octahedron")
            i=8;
        if(s=="Dodecahedron")
            i=12;
        if(s=="Icosahedron")
            i=20;
        sum = sum+i;
    }
    cout << sum << "\n";
    return 0;
}
